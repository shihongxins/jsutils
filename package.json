{
  "name": "@shihongxins/jsutils",
  "version": "1.2.0",
  "description": "Personal Javascript Utils Library.",
  "keywords": [
    "Javascript",
    "Utils"
  ],
  "author": "shihongxins@163.com",
  "homepage": "https://shihongxins.github.io/jsutils/",
  "repository": {
    "type": "git",
    "url": "https://github.com/shihongxins/jsutils"
  },
  "license": "ISC",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.umd.js"
    }
  },
  "type": "module",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepare": "husky install",
    "format": "prettier ./src/ --write",
    "lint": "eslint ./src --ext .ts",
    "test": "jest",
    "build:types": "tsc",
    "bulid": "tsc && rollup -c",
    "commit": "cz",
    "release": "release-it",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "rollup": "^3.29.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-rc.37",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "dayjs": "^1.11.9"
  }
}
